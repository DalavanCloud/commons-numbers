<a class="jxr_linenumber" name="L149" href="#L149">149</a>     <strong class="jxr_keyword">private</strong> EnclosingBall&lt;Euclidean2D, Vector2D&gt; checkDisk(List&lt;Vector2D&gt; points) {
<a class="jxr_linenumber" name="L150" href="#L150">150</a> 
<a class="jxr_linenumber" name="L151" href="#L151">151</a>         WelzlEncloser&lt;Euclidean2D, Vector2D&gt; encloser =
<a class="jxr_linenumber" name="L152" href="#L152">152</a>                 <strong class="jxr_keyword">new</strong> WelzlEncloser&lt;Euclidean2D, Vector2D&gt;(1.0e-10, <strong class="jxr_keyword">new</strong> DiskGenerator());
<a class="jxr_linenumber" name="L153" href="#L153">153</a>         EnclosingBall&lt;Euclidean2D, Vector2D&gt; disk = encloser.enclose(points);
<a class="jxr_linenumber" name="L154" href="#L154">154</a> 
<a class="jxr_linenumber" name="L155" href="#L155">155</a>         <em class="jxr_comment">// all points are enclosed</em>
<a class="jxr_linenumber" name="L156" href="#L156">156</a>         <strong class="jxr_keyword">for</strong> (Vector2D v : points) {
<a class="jxr_linenumber" name="L157" href="#L157">157</a>             Assert.assertTrue(disk.contains(v, 1.0e-10));
<a class="jxr_linenumber" name="L158" href="#L158">158</a>         }
<a class="jxr_linenumber" name="L159" href="#L159">159</a> 
<a class="jxr_linenumber" name="L160" href="#L160">160</a>         <strong class="jxr_keyword">for</strong> (Vector2D v : points) {
<a class="jxr_linenumber" name="L161" href="#L161">161</a>             <strong class="jxr_keyword">boolean</strong> inSupport = false;
<a class="jxr_linenumber" name="L162" href="#L162">162</a>             <strong class="jxr_keyword">for</strong> (Vector2D s : disk.getSupport()) {
<a class="jxr_linenumber" name="L163" href="#L163">163</a>                 <strong class="jxr_keyword">if</strong> (v == s) {
