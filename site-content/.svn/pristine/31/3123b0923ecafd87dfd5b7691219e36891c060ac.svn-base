<a class="jxr_linenumber" name="L106" href="#L106">106</a>     <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">void</strong> checkDisk(List&lt;Vector2D&gt; points, List&lt;Vector2D&gt; refSupport) {
<a class="jxr_linenumber" name="L107" href="#L107">107</a> 
<a class="jxr_linenumber" name="L108" href="#L108">108</a>         EnclosingBall&lt;Euclidean2D, Vector2D&gt; disk = checkDisk(points);
<a class="jxr_linenumber" name="L109" href="#L109">109</a> 
<a class="jxr_linenumber" name="L110" href="#L110">110</a>         <em class="jxr_comment">// compare computed disk with expected disk</em>
<a class="jxr_linenumber" name="L111" href="#L111">111</a>         DiskGenerator generator = <strong class="jxr_keyword">new</strong> DiskGenerator();
<a class="jxr_linenumber" name="L112" href="#L112">112</a>         EnclosingBall&lt;Euclidean2D, Vector2D&gt; expected = generator.ballOnSupport(refSupport);
<a class="jxr_linenumber" name="L113" href="#L113">113</a>         Assert.assertEquals(refSupport.size(), disk.getSupportSize());
<a class="jxr_linenumber" name="L114" href="#L114">114</a>         Assert.assertEquals(expected.getRadius(),        disk.getRadius(),        1.0e-10);
<a class="jxr_linenumber" name="L115" href="#L115">115</a>         Assert.assertEquals(expected.getCenter().getX(), disk.getCenter().getX(), 1.0e-10);
<a class="jxr_linenumber" name="L116" href="#L116">116</a>         Assert.assertEquals(expected.getCenter().getY(), disk.getCenter().getY(), 1.0e-10);
<a class="jxr_linenumber" name="L117" href="#L117">117</a> 
<a class="jxr_linenumber" name="L118" href="#L118">118</a>         <strong class="jxr_keyword">for</strong> (Vector2D s : disk.getSupport()) {
<a class="jxr_linenumber" name="L119" href="#L119">119</a>             <strong class="jxr_keyword">boolean</strong> found = false;
<a class="jxr_linenumber" name="L120" href="#L120">120</a>             <strong class="jxr_keyword">for</strong> (Vector2D rs : refSupport) {
<a class="jxr_linenumber" name="L121" href="#L121">121</a>                 <strong class="jxr_keyword">if</strong> (s == rs) {
<a class="jxr_linenumber" name="L122" href="#L122">122</a>                     found = <strong class="jxr_keyword">true</strong>;
<a class="jxr_linenumber" name="L123" href="#L123">123</a>                 }
<a class="jxr_linenumber" name="L124" href="#L124">124</a>             }
<a class="jxr_linenumber" name="L125" href="#L125">125</a>             Assert.assertTrue(found);
<a class="jxr_linenumber" name="L126" href="#L126">126</a>         }
<a class="jxr_linenumber" name="L127" href="#L127">127</a> 
<a class="jxr_linenumber" name="L128" href="#L128">128</a>         <em class="jxr_comment">// check removing any point of the support disk fails to enclose the point</em>
<a class="jxr_linenumber" name="L129" href="#L129">129</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; disk.getSupportSize(); ++i) {
<a class="jxr_linenumber" name="L130" href="#L130">130</a>             List&lt;Vector2D&gt; reducedSupport = <strong class="jxr_keyword">new</strong> ArrayList&lt;Vector2D&gt;();
<a class="jxr_linenumber" name="L131" href="#L131">131</a>             <strong class="jxr_keyword">int</strong> count = 0;
<a class="jxr_linenumber" name="L132" href="#L132">132</a>             <strong class="jxr_keyword">for</strong> (Vector2D s : disk.getSupport()) {
<a class="jxr_linenumber" name="L133" href="#L133">133</a>                 <strong class="jxr_keyword">if</strong> (count++ != i) {
<a class="jxr_linenumber" name="L134" href="#L134">134</a>                     reducedSupport.add(s);
<a class="jxr_linenumber" name="L135" href="#L135">135</a>                 }
<a class="jxr_linenumber" name="L136" href="#L136">136</a>             }
<a class="jxr_linenumber" name="L137" href="#L137">137</a>             EnclosingBall&lt;Euclidean2D, Vector2D&gt; reducedDisk = generator.ballOnSupport(reducedSupport);
<a class="jxr_linenumber" name="L138" href="#L138">138</a>             <strong class="jxr_keyword">boolean</strong> foundOutside = false;
<a class="jxr_linenumber" name="L139" href="#L139">139</a>             <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> j = 0; j &lt; points.size() &amp;&amp; !foundOutside; ++j) {
<a class="jxr_linenumber" name="L140" href="#L140">140</a>                 <strong class="jxr_keyword">if</strong> (!reducedDisk.contains(points.get(j), 1.0e-10)) {
<a class="jxr_linenumber" name="L141" href="#L141">141</a>                     foundOutside = <strong class="jxr_keyword">true</strong>;
<a class="jxr_linenumber" name="L142" href="#L142">142</a>                 }
<a class="jxr_linenumber" name="L143" href="#L143">143</a>             }
<a class="jxr_linenumber" name="L144" href="#L144">144</a>             Assert.assertTrue(foundOutside);
<a class="jxr_linenumber" name="L145" href="#L145">145</a>         }
<a class="jxr_linenumber" name="L146" href="#L146">146</a> 
<a class="jxr_linenumber" name="L147" href="#L147">147</a>     }
