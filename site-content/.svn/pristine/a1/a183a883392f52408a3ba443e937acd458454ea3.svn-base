<span class="sourceLineNo">025</span><a name="line.25"></a>
<span class="sourceLineNo">026</span><a name="line.26"></a>
<span class="sourceLineNo">027</span>public class FitnessCachingTest {<a name="line.27"></a>
<span class="sourceLineNo">028</span><a name="line.28"></a>
<span class="sourceLineNo">029</span>    // parameters for the GA<a name="line.29"></a>
<span class="sourceLineNo">030</span>    private static final int DIMENSION = 50;<a name="line.30"></a>
<span class="sourceLineNo">031</span>    private static final double CROSSOVER_RATE = 1;<a name="line.31"></a>
<span class="sourceLineNo">032</span>    private static final double MUTATION_RATE = 0.1;<a name="line.32"></a>
<span class="sourceLineNo">033</span>    private static final int TOURNAMENT_ARITY = 5;<a name="line.33"></a>
<span class="sourceLineNo">034</span><a name="line.34"></a>
<span class="sourceLineNo">035</span>    private static final int POPULATION_SIZE = 10;<a name="line.35"></a>
<span class="sourceLineNo">036</span>    private static final int NUM_GENERATIONS = 50;<a name="line.36"></a>
<span class="sourceLineNo">037</span>    private static final double ELITISM_RATE = 0.2;<a name="line.37"></a>
<span class="sourceLineNo">038</span><a name="line.38"></a>
<span class="sourceLineNo">039</span>    // how many times was the fitness computed<a name="line.39"></a>
<span class="sourceLineNo">040</span>    private static int fitnessCalls = 0;<a name="line.40"></a>
<span class="sourceLineNo">041</span><a name="line.41"></a>
<span class="sourceLineNo">042</span><a name="line.42"></a>
<span class="sourceLineNo">043</span>    @Test<a name="line.43"></a>
<span class="sourceLineNo">044</span>    public void testFitnessCaching() {<a name="line.44"></a>
<span class="sourceLineNo">045</span>        // initialize a new genetic algorithm<a name="line.45"></a>
<span class="sourceLineNo">046</span>        GeneticAlgorithm ga = new GeneticAlgorithm(<a name="line.46"></a>
<span class="sourceLineNo">047</span>                new OnePointCrossover&lt;Integer&gt;(),<a name="line.47"></a>
<span class="sourceLineNo">048</span>                CROSSOVER_RATE, // all selected chromosomes will be recombined (=crosssover)<a name="line.48"></a>
<span class="sourceLineNo">049</span>                new BinaryMutation(),<a name="line.49"></a>
<span class="sourceLineNo">050</span>                MUTATION_RATE, // no mutation<a name="line.50"></a>
<span class="sourceLineNo">051</span>                new TournamentSelection(TOURNAMENT_ARITY)<a name="line.51"></a>
<span class="sourceLineNo">052</span>        );<a name="line.52"></a>
<span class="sourceLineNo">053</span><a name="line.53"></a>
<span class="sourceLineNo">054</span>        // initial population<a name="line.54"></a>
<span class="sourceLineNo">055</span>        Population initial = randomPopulation();<a name="line.55"></a>
<span class="sourceLineNo">056</span>        // stopping conditions<a name="line.56"></a>
<span class="sourceLineNo">057</span>        StoppingCondition stopCond = new FixedGenerationCount(NUM_GENERATIONS);<a name="line.57"></a>
<span class="sourceLineNo">058</span><a name="line.58"></a>
<span class="sourceLineNo">059</span>        // run the algorithm<a name="line.59"></a>
<span class="sourceLineNo">060</span>        ga.evolve(initial, stopCond);<a name="line.60"></a>
<span class="sourceLineNo">061</span><a name="line.61"></a>
<span class="sourceLineNo">062</span>        int neededCalls =<a name="line.62"></a>
<span class="sourceLineNo">063</span>            POPULATION_SIZE /*initial population*/ +<a name="line.63"></a>
<span class="sourceLineNo">064</span>            (NUM_GENERATIONS - 1) /*for each population*/ * (int)(POPULATION_SIZE * (1.0 - ELITISM_RATE)) /*some chromosomes are copied*/<a name="line.64"></a>
<span class="sourceLineNo">065</span>            ;<a name="line.65"></a>
<span class="sourceLineNo">066</span>        Assert.assertTrue(fitnessCalls &lt;= neededCalls); // some chromosomes after crossover may be the same os old ones<a name="line.66"></a>
<span class="sourceLineNo">067</span>    }<a name="line.67"></a>
<span class="sourceLineNo">068</span><a name="line.68"></a>
<span class="sourceLineNo">069</span><a name="line.69"></a>
<span class="sourceLineNo">070</span>    /**<a name="line.70"></a>
<span class="sourceLineNo">071</span>     * Initializes a random population.<a name="line.71"></a>
<span class="sourceLineNo">072</span>     */<a name="line.72"></a>
<span class="sourceLineNo">073</span>    private static ElitisticListPopulation randomPopulation() {<a name="line.73"></a>
<span class="sourceLineNo">074</span>        List&lt;Chromosome&gt; popList = new LinkedList&lt;Chromosome&gt;();<a name="line.74"></a>
<span class="sourceLineNo">075</span><a name="line.75"></a>
<span class="sourceLineNo">076</span>        for (int i=0; i&lt;POPULATION_SIZE; i++) {<a name="line.76"></a>
<span class="sourceLineNo">077</span>            BinaryChromosome randChrom = new DummyCountingBinaryChromosome(BinaryChromosome.randomBinaryRepresentation(DIMENSION));<a name="line.77"></a>
<span class="sourceLineNo">078</span>            popList.add(randChrom);<a name="line.78"></a>
<span class="sourceLineNo">079</span>        }<a name="line.79"></a>
<span class="sourceLineNo">080</span>        return new ElitisticListPopulation(popList, popList.size(), ELITISM_RATE);<a name="line.80"></a>
<span class="sourceLineNo">081</span>    }<a name="line.81"></a>
<span class="sourceLineNo">082</span><a name="line.82"></a>
<span class="sourceLineNo">083</span>    private static class DummyCountingBinaryChromosome extends DummyBinaryChromosome {<a name="line.83"></a>
<span class="sourceLineNo">084</span><a name="line.84"></a>
<span class="sourceLineNo">085</span>        public DummyCountingBinaryChromosome(List&lt;Integer&gt; representation) {<a name="line.85"></a>
<span class="sourceLineNo">086</span>            super(representation);<a name="line.86"></a>
<span class="sourceLineNo">087</span>        }<a name="line.87"></a>
<span class="sourceLineNo">088</span><a name="line.88"></a>
<span class="sourceLineNo">089</span>        @Override<a name="line.89"></a>
<span class="sourceLineNo">090</span>        public double fitness() {<a name="line.90"></a>
<span class="sourceLineNo">091</span>            fitnessCalls++;<a name="line.91"></a>
<span class="sourceLineNo">092</span>            return 0;<a name="line.92"></a>
<span class="sourceLineNo">093</span>        }<a name="line.93"></a>
<span class="sourceLineNo">094</span>    }<a name="line.94"></a>
