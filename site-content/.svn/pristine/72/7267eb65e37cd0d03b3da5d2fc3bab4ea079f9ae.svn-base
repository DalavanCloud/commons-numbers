<a class="jxr_linenumber" name="L700" href="#L700">700</a> <em class="jxr_javadoccomment">         * if the matrices dimensions do not match.</em>
<a class="jxr_linenumber" name="L701" href="#L701">701</a> <em class="jxr_javadoccomment">         */</em>
<a class="jxr_linenumber" name="L702" href="#L702">702</a>         <strong class="jxr_keyword">public</strong> <a href="../../../../../org/apache/commons/math3/linear/RealVector.html">RealVector</a> solve(<strong class="jxr_keyword">final</strong> <a href="../../../../../org/apache/commons/math3/linear/RealVector.html">RealVector</a> b) {
