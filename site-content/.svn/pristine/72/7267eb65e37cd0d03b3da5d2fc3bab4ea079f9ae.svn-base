<a class="jxr_linenumber" name="L541" href="#L541">541</a> <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L542" href="#L542">542</a>     <strong class="jxr_keyword">public</strong> <a href="../../../../../org/apache/commons/math3/linear/RealMatrix.html">RealMatrix</a> getV() {
<a class="jxr_linenumber" name="L543" href="#L543">543</a>         <em class="jxr_comment">// return the cached matrix</em>
