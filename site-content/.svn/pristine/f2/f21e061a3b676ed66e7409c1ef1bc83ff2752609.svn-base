<a class="jxr_linenumber" name="L38" href="#L38">38</a>          Assert.assertEquals(Region.Location.INSIDE,   arc.checkPoint(8.7));
<a class="jxr_linenumber" name="L39" href="#L39">39</a>          Assert.assertEquals(Region.Location.INSIDE,   arc.checkPoint(3.0));
