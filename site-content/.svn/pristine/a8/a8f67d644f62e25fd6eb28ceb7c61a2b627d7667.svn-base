<a class="jxr_linenumber" name="L379" href="#L379">379</a> <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L380" href="#L380">380</a>     <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">boolean</strong> containsKey(<strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> key, <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> index) {
<a class="jxr_linenumber" name="L381" href="#L381">381</a>         <strong class="jxr_keyword">return</strong> (key != 0 || states[index] == FULL) &amp;&amp; keys[index] == key;
