<a class="jxr_linenumber" name="L473" href="#L473">473</a>     <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">int</strong> hashOf(<strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> key) {
