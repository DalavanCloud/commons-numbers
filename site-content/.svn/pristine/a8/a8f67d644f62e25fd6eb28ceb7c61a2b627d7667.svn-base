<a class="jxr_linenumber" name="L321" href="#L321">321</a>     <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">int</strong> probe(<strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> perturb, <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> j) {
