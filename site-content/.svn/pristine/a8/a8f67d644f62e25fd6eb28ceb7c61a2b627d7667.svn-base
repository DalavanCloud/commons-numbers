<a class="jxr_linenumber" name="L330" href="#L330">330</a>     <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">int</strong> changeIndexSign(<strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> index) {
