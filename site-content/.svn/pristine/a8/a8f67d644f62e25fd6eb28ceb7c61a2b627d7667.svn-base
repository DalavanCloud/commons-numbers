<a class="jxr_linenumber" name="L405" href="#L405">405</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">double</strong> put(<strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> key, <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> value) {
<a class="jxr_linenumber" name="L406" href="#L406">406</a>         <strong class="jxr_keyword">int</strong> index = findInsertionIndex(key);
<a class="jxr_linenumber" name="L407" href="#L407">407</a>         <strong class="jxr_keyword">double</strong> previous = missingEntries;
<a class="jxr_linenumber" name="L408" href="#L408">408</a>         <strong class="jxr_keyword">boolean</strong> newMapping = <strong class="jxr_keyword">true</strong>;
<a class="jxr_linenumber" name="L409" href="#L409">409</a>         <strong class="jxr_keyword">if</strong> (index &lt; 0) {
