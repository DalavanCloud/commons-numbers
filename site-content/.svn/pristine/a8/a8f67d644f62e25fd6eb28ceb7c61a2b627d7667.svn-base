<a class="jxr_linenumber" name="L431" href="#L431">431</a>     <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">void</strong> growTable() {
<a class="jxr_linenumber" name="L432" href="#L432">432</a> 
<a class="jxr_linenumber" name="L433" href="#L433">433</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> oldLength      = states.length;
<a class="jxr_linenumber" name="L434" href="#L434">434</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong>[] oldKeys      = keys;
<a class="jxr_linenumber" name="L435" href="#L435">435</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong>[] oldValues = values;
<a class="jxr_linenumber" name="L436" href="#L436">436</a>         <strong class="jxr_keyword">final</strong> byte[] oldStates   = states;
<a class="jxr_linenumber" name="L437" href="#L437">437</a> 
<a class="jxr_linenumber" name="L438" href="#L438">438</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> newLength = RESIZE_MULTIPLIER * oldLength;
<a class="jxr_linenumber" name="L439" href="#L439">439</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong>[] newKeys = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">int</strong>[newLength];
<a class="jxr_linenumber" name="L440" href="#L440">440</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong>[] newValues = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[newLength];
<a class="jxr_linenumber" name="L441" href="#L441">441</a>         <strong class="jxr_keyword">final</strong> byte[] newStates = <strong class="jxr_keyword">new</strong> byte[newLength];
<a class="jxr_linenumber" name="L442" href="#L442">442</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> newMask = newLength - 1;
<a class="jxr_linenumber" name="L443" href="#L443">443</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; oldLength; ++i) {
<a class="jxr_linenumber" name="L444" href="#L444">444</a>             <strong class="jxr_keyword">if</strong> (oldStates[i] == FULL) {
<a class="jxr_linenumber" name="L445" href="#L445">445</a>                 <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> key = oldKeys[i];
