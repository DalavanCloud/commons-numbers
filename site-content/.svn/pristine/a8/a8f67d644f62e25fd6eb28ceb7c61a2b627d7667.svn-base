<a class="jxr_linenumber" name="L348" href="#L348">348</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">double</strong> remove(<strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> key) {
<a class="jxr_linenumber" name="L349" href="#L349">349</a> 
