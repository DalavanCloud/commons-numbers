<a class="jxr_linenumber" name="L358" href="#L358">358</a>         }
<a class="jxr_linenumber" name="L359" href="#L359">359</a> 
<a class="jxr_linenumber" name="L360" href="#L360">360</a>         <strong class="jxr_keyword">int</strong> j = index;
<a class="jxr_linenumber" name="L361" href="#L361">361</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> perturb = perturb(hash); states[index] != FREE; perturb &gt;&gt;= PERTURB_SHIFT) {
<a class="jxr_linenumber" name="L362" href="#L362">362</a>             j = probe(perturb, j);
