<a class="jxr_linenumber" name="L127" href="#L127">127</a>     <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">void</strong> checkSphere(List&lt;Vector3D&gt; points, <strong class="jxr_keyword">double</strong> refRadius) {
<a class="jxr_linenumber" name="L128" href="#L128">128</a> 
<a class="jxr_linenumber" name="L129" href="#L129">129</a>         EnclosingBall&lt;Euclidean3D, Vector3D&gt; sphere = checkSphere(points);
<a class="jxr_linenumber" name="L130" href="#L130">130</a> 
<a class="jxr_linenumber" name="L131" href="#L131">131</a>         <em class="jxr_comment">// compare computed sphere with bounding sphere</em>
<a class="jxr_linenumber" name="L132" href="#L132">132</a>         Assert.assertTrue(sphere.getRadius() &lt;= refRadius);
<a class="jxr_linenumber" name="L133" href="#L133">133</a> 
<a class="jxr_linenumber" name="L134" href="#L134">134</a>         <em class="jxr_comment">// check removing any point of the support Sphere fails to enclose the point</em>
<a class="jxr_linenumber" name="L135" href="#L135">135</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; sphere.getSupportSize(); ++i) {
<a class="jxr_linenumber" name="L136" href="#L136">136</a>             List&lt;Vector3D&gt; reducedSupport = <strong class="jxr_keyword">new</strong> ArrayList&lt;Vector3D&gt;();
<a class="jxr_linenumber" name="L137" href="#L137">137</a>             <strong class="jxr_keyword">int</strong> count = 0;
<a class="jxr_linenumber" name="L138" href="#L138">138</a>             <strong class="jxr_keyword">for</strong> (Vector3D s : sphere.getSupport()) {
<a class="jxr_linenumber" name="L139" href="#L139">139</a>                 <strong class="jxr_keyword">if</strong> (count++ != i) {
<a class="jxr_linenumber" name="L140" href="#L140">140</a>                     reducedSupport.add(s);
<a class="jxr_linenumber" name="L141" href="#L141">141</a>                 }
<a class="jxr_linenumber" name="L142" href="#L142">142</a>             }
<a class="jxr_linenumber" name="L143" href="#L143">143</a>             EnclosingBall&lt;Euclidean3D, Vector3D&gt; reducedSphere =
<a class="jxr_linenumber" name="L144" href="#L144">144</a>                     <strong class="jxr_keyword">new</strong> SphereGenerator().ballOnSupport(reducedSupport);
<a class="jxr_linenumber" name="L145" href="#L145">145</a>             <strong class="jxr_keyword">boolean</strong> foundOutside = false;
<a class="jxr_linenumber" name="L146" href="#L146">146</a>             <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> j = 0; j &lt; points.size() &amp;&amp; !foundOutside; ++j) {
<a class="jxr_linenumber" name="L147" href="#L147">147</a>                 <strong class="jxr_keyword">if</strong> (!reducedSphere.contains(points.get(j), 1.0e-10)) {
<a class="jxr_linenumber" name="L148" href="#L148">148</a>                     foundOutside = <strong class="jxr_keyword">true</strong>;
<a class="jxr_linenumber" name="L149" href="#L149">149</a>                 }
<a class="jxr_linenumber" name="L150" href="#L150">150</a>             }
<a class="jxr_linenumber" name="L151" href="#L151">151</a>             Assert.assertTrue(foundOutside);
<a class="jxr_linenumber" name="L152" href="#L152">152</a>         }
<a class="jxr_linenumber" name="L153" href="#L153">153</a> 
<a class="jxr_linenumber" name="L154" href="#L154">154</a>     }
<a class="jxr_linenumber" name="L155" href="#L155">155</a> 
<a class="jxr_linenumber" name="L156" href="#L156">156</a>     <strong class="jxr_keyword">private</strong> EnclosingBall&lt;Euclidean3D, Vector3D&gt; checkSphere(List&lt;Vector3D&gt; points) {
<a class="jxr_linenumber" name="L157" href="#L157">157</a> 
<a class="jxr_linenumber" name="L158" href="#L158">158</a>         WelzlEncloser&lt;Euclidean3D, Vector3D&gt; encloser =
<a class="jxr_linenumber" name="L159" href="#L159">159</a>                 <strong class="jxr_keyword">new</strong> WelzlEncloser&lt;Euclidean3D, Vector3D&gt;(1.0e-10, <strong class="jxr_keyword">new</strong> SphereGenerator());
<a class="jxr_linenumber" name="L160" href="#L160">160</a>         EnclosingBall&lt;Euclidean3D, Vector3D&gt; Sphere = encloser.enclose(points);
<a class="jxr_linenumber" name="L161" href="#L161">161</a> 
<a class="jxr_linenumber" name="L162" href="#L162">162</a>         <em class="jxr_comment">// all points are enclosed</em>
<a class="jxr_linenumber" name="L163" href="#L163">163</a>         <strong class="jxr_keyword">for</strong> (Vector3D v : points) {
<a class="jxr_linenumber" name="L164" href="#L164">164</a>             Assert.assertTrue(Sphere.contains(v, 1.0e-10));
<a class="jxr_linenumber" name="L165" href="#L165">165</a>         }
<a class="jxr_linenumber" name="L166" href="#L166">166</a> 
<a class="jxr_linenumber" name="L167" href="#L167">167</a>         <strong class="jxr_keyword">for</strong> (Vector3D v : points) {
<a class="jxr_linenumber" name="L168" href="#L168">168</a>             <strong class="jxr_keyword">boolean</strong> inSupport = false;
<a class="jxr_linenumber" name="L169" href="#L169">169</a>             <strong class="jxr_keyword">for</strong> (Vector3D s : Sphere.getSupport()) {
<a class="jxr_linenumber" name="L170" href="#L170">170</a>                 <strong class="jxr_keyword">if</strong> (v == s) {
<a class="jxr_linenumber" name="L171" href="#L171">171</a>                     inSupport = <strong class="jxr_keyword">true</strong>;
<a class="jxr_linenumber" name="L172" href="#L172">172</a>                 }
<a class="jxr_linenumber" name="L173" href="#L173">173</a>             }
<a class="jxr_linenumber" name="L174" href="#L174">174</a>             <strong class="jxr_keyword">if</strong> (inSupport) {
<a class="jxr_linenumber" name="L175" href="#L175">175</a>                 <em class="jxr_comment">// points on the support should be outside of reduced ball</em>
<a class="jxr_linenumber" name="L176" href="#L176">176</a>                 Assert.assertFalse(Sphere.contains(v, -0.001));
<a class="jxr_linenumber" name="L177" href="#L177">177</a>             }
<a class="jxr_linenumber" name="L178" href="#L178">178</a>         }
<a class="jxr_linenumber" name="L179" href="#L179">179</a> 
<a class="jxr_linenumber" name="L180" href="#L180">180</a>         <strong class="jxr_keyword">return</strong> Sphere;
<a class="jxr_linenumber" name="L181" href="#L181">181</a> 
<a class="jxr_linenumber" name="L182" href="#L182">182</a>     }
