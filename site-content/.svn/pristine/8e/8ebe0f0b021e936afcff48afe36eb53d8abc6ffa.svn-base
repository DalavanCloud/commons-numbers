<a class="jxr_linenumber" name="L104" href="#L104">104</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testLargeSamples() <strong class="jxr_keyword">throws</strong> IOException {
<a class="jxr_linenumber" name="L105" href="#L105">105</a>         RandomGenerator random = <strong class="jxr_keyword">new</strong> Well1024a(0x35ddecfc78131e1dl);
<a class="jxr_linenumber" name="L106" href="#L106">106</a>         <strong class="jxr_keyword">final</strong> UnitSphereRandomVectorGenerator sr = <strong class="jxr_keyword">new</strong> UnitSphereRandomVectorGenerator(3, random);
<a class="jxr_linenumber" name="L107" href="#L107">107</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> k = 0; k &lt; 50; ++k) {
<a class="jxr_linenumber" name="L108" href="#L108">108</a> 
<a class="jxr_linenumber" name="L109" href="#L109">109</a>             <em class="jxr_comment">// define the reference sphere we want to compute</em>
<a class="jxr_linenumber" name="L110" href="#L110">110</a>             <strong class="jxr_keyword">double</strong> d = 25 * random.nextDouble();
<a class="jxr_linenumber" name="L111" href="#L111">111</a>             <strong class="jxr_keyword">double</strong> refRadius = 10 * random.nextDouble();
<a class="jxr_linenumber" name="L112" href="#L112">112</a>             Vector3D refCenter = <strong class="jxr_keyword">new</strong> Vector3D(d, <strong class="jxr_keyword">new</strong> Vector3D(sr.nextVector()));
<a class="jxr_linenumber" name="L113" href="#L113">113</a>             <em class="jxr_comment">// set up a large sample inside the reference sphere</em>
<a class="jxr_linenumber" name="L114" href="#L114">114</a>             <strong class="jxr_keyword">int</strong> nbPoints = random.nextInt(1000);
<a class="jxr_linenumber" name="L115" href="#L115">115</a>             List&lt;Vector3D&gt; points = <strong class="jxr_keyword">new</strong> ArrayList&lt;Vector3D&gt;();
<a class="jxr_linenumber" name="L116" href="#L116">116</a>             <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; nbPoints; ++i) {
<a class="jxr_linenumber" name="L117" href="#L117">117</a>                 <strong class="jxr_keyword">double</strong> r = refRadius * random.nextDouble();
<a class="jxr_linenumber" name="L118" href="#L118">118</a>                 points.add(<strong class="jxr_keyword">new</strong> Vector3D(1.0, refCenter, r, <strong class="jxr_keyword">new</strong> Vector3D(sr.nextVector())));
<a class="jxr_linenumber" name="L119" href="#L119">119</a>             }
<a class="jxr_linenumber" name="L120" href="#L120">120</a> 
<a class="jxr_linenumber" name="L121" href="#L121">121</a>             <em class="jxr_comment">// test we find a sphere at most as large as the one used for random drawings</em>
<a class="jxr_linenumber" name="L122" href="#L122">122</a>             checkSphere(points, refRadius);
<a class="jxr_linenumber" name="L123" href="#L123">123</a> 
<a class="jxr_linenumber" name="L124" href="#L124">124</a>         }
<a class="jxr_linenumber" name="L125" href="#L125">125</a>     }
