<a class="jxr_linenumber" name="L622" href="#L622">622</a>     @Test
<a class="jxr_linenumber" name="L623" href="#L623">623</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testAccurateCrossProduct() {
<a class="jxr_linenumber" name="L624" href="#L624">624</a>         <em class="jxr_comment">// the vectors u1 and u2 are nearly but not exactly anti-parallel</em>
<a class="jxr_linenumber" name="L625" href="#L625">625</a>         <em class="jxr_comment">// (7.31e-16 degrees from 180 degrees) naive cross product (i.e.</em>
<a class="jxr_linenumber" name="L626" href="#L626">626</a>         <em class="jxr_comment">// computing u1.x * u2.x + u1.y * u2.y + u1.z * u2.z</em>
<a class="jxr_linenumber" name="L627" href="#L627">627</a>         <em class="jxr_comment">// leads to a result of   [0.0009765, -0.0001220, -0.0039062],</em>
<a class="jxr_linenumber" name="L628" href="#L628">628</a>         <em class="jxr_comment">// instead of the correct [0.0006913, -0.0001254, -0.0007909]</em>
<a class="jxr_linenumber" name="L629" href="#L629">629</a>         <strong class="jxr_keyword">final</strong> FieldVector3D&lt;DerivativeStructure&gt; u1 = createVector(-1321008684645961.0 /   268435456.0,
<a class="jxr_linenumber" name="L630" href="#L630">630</a>                                          -5774608829631843.0 /   268435456.0,
<a class="jxr_linenumber" name="L631" href="#L631">631</a>                                          -7645843051051357.0 /  8589934592.0, 3);
<a class="jxr_linenumber" name="L632" href="#L632">632</a>         <strong class="jxr_keyword">final</strong> FieldVector3D&lt;DerivativeStructure&gt; u2 = createVector( 1796571811118507.0 /  2147483648.0,
<a class="jxr_linenumber" name="L633" href="#L633">633</a>                                           7853468008299307.0 /  2147483648.0,
<a class="jxr_linenumber" name="L634" href="#L634">634</a>                                           2599586637357461.0 / 17179869184.0, 3);
<a class="jxr_linenumber" name="L635" href="#L635">635</a>         <strong class="jxr_keyword">final</strong> FieldVector3D&lt;DerivativeStructure&gt; u3 = createVector(12753243807587107.0 / 18446744073709551616.0,
<a class="jxr_linenumber" name="L636" href="#L636">636</a>                                          -2313766922703915.0 / 18446744073709551616.0,
<a class="jxr_linenumber" name="L637" href="#L637">637</a>                                           -227970081415313.0 /   288230376151711744.0, 3);
<a class="jxr_linenumber" name="L638" href="#L638">638</a>         FieldVector3D&lt;DerivativeStructure&gt; cNaive = <strong class="jxr_keyword">new</strong> FieldVector3D&lt;DerivativeStructure&gt;(u1.getY().multiply(u2.getZ()).subtract(u1.getZ().multiply(u2.getY())),
<a class="jxr_linenumber" name="L639" href="#L639">639</a>                                        u1.getZ().multiply(u2.getX()).subtract(u1.getX().multiply(u2.getZ())),
<a class="jxr_linenumber" name="L640" href="#L640">640</a>                                        u1.getX().multiply(u2.getY()).subtract(u1.getY().multiply(u2.getX())));
<a class="jxr_linenumber" name="L641" href="#L641">641</a>         FieldVector3D&lt;DerivativeStructure&gt; cAccurate = FieldVector3D.crossProduct(u1, u2);
<a class="jxr_linenumber" name="L642" href="#L642">642</a>         Assert.assertTrue(FieldVector3D.distance(u3, cNaive).getReal() &gt; 2.9 * u3.getNorm().getReal());
<a class="jxr_linenumber" name="L643" href="#L643">643</a>         Assert.assertEquals(0.0, FieldVector3D.distance(u3, cAccurate).getReal(), 1.0e-30 * cAccurate.getNorm().getReal());
<a class="jxr_linenumber" name="L644" href="#L644">644</a>     }
