<a class="jxr_linenumber" name="L646" href="#L646">646</a>     @Test
<a class="jxr_linenumber" name="L647" href="#L647">647</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testCrossProduct() {
<a class="jxr_linenumber" name="L648" href="#L648">648</a>         <em class="jxr_comment">// we compare accurate versus naive cross product implementations</em>
<a class="jxr_linenumber" name="L649" href="#L649">649</a>         <em class="jxr_comment">// on regular vectors (i.e. not extreme cases like in the previous test)</em>
<a class="jxr_linenumber" name="L650" href="#L650">650</a>         Well1024a random = <strong class="jxr_keyword">new</strong> Well1024a(885362227452043214l);
<a class="jxr_linenumber" name="L651" href="#L651">651</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; 10000; ++i) {
<a class="jxr_linenumber" name="L652" href="#L652">652</a>             <strong class="jxr_keyword">double</strong> ux = random.nextDouble();
<a class="jxr_linenumber" name="L653" href="#L653">653</a>             <strong class="jxr_keyword">double</strong> uy = random.nextDouble();
<a class="jxr_linenumber" name="L654" href="#L654">654</a>             <strong class="jxr_keyword">double</strong> uz = random.nextDouble();
<a class="jxr_linenumber" name="L655" href="#L655">655</a>             <strong class="jxr_keyword">double</strong> vx = random.nextDouble();
<a class="jxr_linenumber" name="L656" href="#L656">656</a>             <strong class="jxr_keyword">double</strong> vy = random.nextDouble();
<a class="jxr_linenumber" name="L657" href="#L657">657</a>             <strong class="jxr_keyword">double</strong> vz = random.nextDouble();
<a class="jxr_linenumber" name="L658" href="#L658">658</a>             Vector3D cNaive = <strong class="jxr_keyword">new</strong> Vector3D(uy * vz - uz * vy, uz * vx - ux * vz, ux * vy - uy * vx);
<a class="jxr_linenumber" name="L659" href="#L659">659</a> 
<a class="jxr_linenumber" name="L660" href="#L660">660</a>             FieldVector3D&lt;DerivativeStructure&gt; uds = createVector(ux, uy, uz, 3);
<a class="jxr_linenumber" name="L661" href="#L661">661</a>             FieldVector3D&lt;DerivativeStructure&gt; vds = createVector(vx, vy, vz, 3);
<a class="jxr_linenumber" name="L662" href="#L662">662</a>             Vector3D v = <strong class="jxr_keyword">new</strong> Vector3D(vx, vy, vz);
<a class="jxr_linenumber" name="L663" href="#L663">663</a> 
<a class="jxr_linenumber" name="L664" href="#L664">664</a>             checkVector(FieldVector3D.crossProduct(uds, vds),
<a class="jxr_linenumber" name="L665" href="#L665">665</a>                         cNaive.getX(), cNaive.getY(), cNaive.getZ(),
<a class="jxr_linenumber" name="L666" href="#L666">666</a>                         0, vz - uz, uy - vy,
<a class="jxr_linenumber" name="L667" href="#L667">667</a>                         uz - vz, 0, vx - ux,
<a class="jxr_linenumber" name="L668" href="#L668">668</a>                         vy - uy, ux - vx, 0);
<a class="jxr_linenumber" name="L669" href="#L669">669</a> 
<a class="jxr_linenumber" name="L670" href="#L670">670</a>             checkVector(FieldVector3D.crossProduct(uds, v),
<a class="jxr_linenumber" name="L671" href="#L671">671</a>                         cNaive.getX(), cNaive.getY(), cNaive.getZ(),
<a class="jxr_linenumber" name="L672" href="#L672">672</a>                           0,  vz, -vy,
<a class="jxr_linenumber" name="L673" href="#L673">673</a>                         -vz,   0,  vx,
<a class="jxr_linenumber" name="L674" href="#L674">674</a>                          vy, -vx,   0);
<a class="jxr_linenumber" name="L675" href="#L675">675</a> 
<a class="jxr_linenumber" name="L676" href="#L676">676</a>         }
<a class="jxr_linenumber" name="L677" href="#L677">677</a>     }
