<a class="jxr_linenumber" name="L34" href="#L34">34</a>      @Test
<a class="jxr_linenumber" name="L35" href="#L35">35</a>      <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testSinFunction() {
<a class="jxr_linenumber" name="L36" href="#L36">36</a>          UnivariateFunction f = <strong class="jxr_keyword">new</strong> Sin();
<a class="jxr_linenumber" name="L37" href="#L37">37</a>          BaseAbstractUnivariateIntegrator integrator = <strong class="jxr_keyword">new</strong> LegendreGaussIntegrator(5, 1.0e-14, 1.0e-10, 2, 15);
<a class="jxr_linenumber" name="L38" href="#L38">38</a>          <strong class="jxr_keyword">double</strong> min, max, expected, result, tolerance;
<a class="jxr_linenumber" name="L39" href="#L39">39</a>  
<a class="jxr_linenumber" name="L40" href="#L40">40</a>          min = 0; max = FastMath.PI; expected = 2;
<a class="jxr_linenumber" name="L41" href="#L41">41</a>          tolerance = FastMath.max(integrator.getAbsoluteAccuracy(),
<a class="jxr_linenumber" name="L42" href="#L42">42</a>                               FastMath.abs(expected * integrator.getRelativeAccuracy()));
<a class="jxr_linenumber" name="L43" href="#L43">43</a>          result = integrator.integrate(10000, f, min, max);
<a class="jxr_linenumber" name="L44" href="#L44">44</a>          Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L45" href="#L45">45</a>  
<a class="jxr_linenumber" name="L46" href="#L46">46</a>          min = -FastMath.PI/3; max = 0; expected = -0.5;
<a class="jxr_linenumber" name="L47" href="#L47">47</a>          tolerance = FastMath.max(integrator.getAbsoluteAccuracy(),
<a class="jxr_linenumber" name="L48" href="#L48">48</a>                  FastMath.abs(expected * integrator.getRelativeAccuracy()));
<a class="jxr_linenumber" name="L49" href="#L49">49</a>          result = integrator.integrate(10000, f, min, max);
<a class="jxr_linenumber" name="L50" href="#L50">50</a>          Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L51" href="#L51">51</a>      }
