<a class="jxr_linenumber" name="L54" href="#L54">54</a>      <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testQuinticFunction() {
<a class="jxr_linenumber" name="L55" href="#L55">55</a>          UnivariateFunction f = <strong class="jxr_keyword">new</strong> <a href="../../../../../../org/apache/commons/math3/analysis/QuinticFunction.html">QuinticFunction</a>();
<a class="jxr_linenumber" name="L56" href="#L56">56</a>          UnivariateIntegrator integrator =
<a class="jxr_linenumber" name="L57" href="#L57">57</a>                  <strong class="jxr_keyword">new</strong> LegendreGaussIntegrator(3,
<a class="jxr_linenumber" name="L58" href="#L58">58</a>                                              BaseAbstractUnivariateIntegrator.DEFAULT_RELATIVE_ACCURACY,
<a class="jxr_linenumber" name="L59" href="#L59">59</a>                                              BaseAbstractUnivariateIntegrator.DEFAULT_ABSOLUTE_ACCURACY,
<a class="jxr_linenumber" name="L60" href="#L60">60</a>                                              BaseAbstractUnivariateIntegrator.DEFAULT_MIN_ITERATIONS_COUNT,
<a class="jxr_linenumber" name="L61" href="#L61">61</a>                                              64);
<a class="jxr_linenumber" name="L62" href="#L62">62</a>          <strong class="jxr_keyword">double</strong> min, max, expected, result;
<a class="jxr_linenumber" name="L63" href="#L63">63</a>  
<a class="jxr_linenumber" name="L64" href="#L64">64</a>          min = 0; max = 1; expected = -1.0/48;
<a class="jxr_linenumber" name="L65" href="#L65">65</a>          result = integrator.integrate(10000, f, min, max);
<a class="jxr_linenumber" name="L66" href="#L66">66</a>          Assert.assertEquals(expected, result, 1.0e-16);
<a class="jxr_linenumber" name="L67" href="#L67">67</a>  
<a class="jxr_linenumber" name="L68" href="#L68">68</a>          min = 0; max = 0.5; expected = 11.0/768;
<a class="jxr_linenumber" name="L69" href="#L69">69</a>          result = integrator.integrate(10000, f, min, max);
<a class="jxr_linenumber" name="L70" href="#L70">70</a>          Assert.assertEquals(expected, result, 1.0e-16);
<a class="jxr_linenumber" name="L71" href="#L71">71</a>  
<a class="jxr_linenumber" name="L72" href="#L72">72</a>          min = -1; max = 4; expected = 2048/3.0 - 78 + 1.0/48;
<a class="jxr_linenumber" name="L73" href="#L73">73</a>          result = integrator.integrate(10000, f, min, max);
<a class="jxr_linenumber" name="L74" href="#L74">74</a>          Assert.assertEquals(expected, result, 1.0e-16);
<a class="jxr_linenumber" name="L75" href="#L75">75</a>      }
<a class="jxr_linenumber" name="L76" href="#L76">76</a>  
<a class="jxr_linenumber" name="L77" href="#L77">77</a>      @Test
<a class="jxr_linenumber" name="L78" href="#L78">78</a>      <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testExactIntegration() {
<a class="jxr_linenumber" name="L79" href="#L79">79</a>          Random random = <strong class="jxr_keyword">new</strong> Random(86343623467878363l);
<a class="jxr_linenumber" name="L80" href="#L80">80</a>          <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> n = 2; n &lt; 6; ++n) {
<a class="jxr_linenumber" name="L81" href="#L81">81</a>              LegendreGaussIntegrator integrator =
<a class="jxr_linenumber" name="L82" href="#L82">82</a>                  <strong class="jxr_keyword">new</strong> LegendreGaussIntegrator(n,
<a class="jxr_linenumber" name="L83" href="#L83">83</a>                                              BaseAbstractUnivariateIntegrator.DEFAULT_RELATIVE_ACCURACY,
<a class="jxr_linenumber" name="L84" href="#L84">84</a>                                              BaseAbstractUnivariateIntegrator.DEFAULT_ABSOLUTE_ACCURACY,
<a class="jxr_linenumber" name="L85" href="#L85">85</a>                                              BaseAbstractUnivariateIntegrator.DEFAULT_MIN_ITERATIONS_COUNT,
<a class="jxr_linenumber" name="L86" href="#L86">86</a>                                              64);
<a class="jxr_linenumber" name="L87" href="#L87">87</a>  
<a class="jxr_linenumber" name="L88" href="#L88">88</a>              <em class="jxr_comment">// an n points Gauss-Legendre integrator integrates 2n-1 degree polynoms exactly</em>
<a class="jxr_linenumber" name="L89" href="#L89">89</a>              <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> degree = 0; degree &lt;= 2 * n - 1; ++degree) {
<a class="jxr_linenumber" name="L90" href="#L90">90</a>                  <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; 10; ++i) {
<a class="jxr_linenumber" name="L91" href="#L91">91</a>                      <strong class="jxr_keyword">double</strong>[] coeff = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[degree + 1];
<a class="jxr_linenumber" name="L92" href="#L92">92</a>                      <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> k = 0; k &lt; coeff.length; ++k) {
<a class="jxr_linenumber" name="L93" href="#L93">93</a>                          coeff[k] = 2 * random.nextDouble() - 1;
<a class="jxr_linenumber" name="L94" href="#L94">94</a>                      }
<a class="jxr_linenumber" name="L95" href="#L95">95</a>                      PolynomialFunction p = <strong class="jxr_keyword">new</strong> PolynomialFunction(coeff);
<a class="jxr_linenumber" name="L96" href="#L96">96</a>                      <strong class="jxr_keyword">double</strong> result    = integrator.integrate(10000, p, -5.0, 15.0);
<a class="jxr_linenumber" name="L97" href="#L97">97</a>                      <strong class="jxr_keyword">double</strong> reference = exactIntegration(p, -5.0, 15.0);
<a class="jxr_linenumber" name="L98" href="#L98">98</a>                      Assert.assertEquals(n + <span class="jxr_string">" "</span> + degree + <span class="jxr_string">" "</span> + i, reference, result, 1.0e-12 * (1.0 + FastMath.abs(reference)));
<a class="jxr_linenumber" name="L99" href="#L99">99</a>                  }
<a class="jxr_linenumber" name="L100" href="#L100">100</a>             }
<a class="jxr_linenumber" name="L101" href="#L101">101</a> 
<a class="jxr_linenumber" name="L102" href="#L102">102</a>         }
<a class="jxr_linenumber" name="L103" href="#L103">103</a>     }
<a class="jxr_linenumber" name="L104" href="#L104">104</a> 
<a class="jxr_linenumber" name="L105" href="#L105">105</a>     @Test
<a class="jxr_linenumber" name="L106" href="#L106">106</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testIssue464() {
<a class="jxr_linenumber" name="L107" href="#L107">107</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> value = 0.2;
<a class="jxr_linenumber" name="L108" href="#L108">108</a>         UnivariateFunction f = <strong class="jxr_keyword">new</strong> UnivariateFunction() {
<a class="jxr_linenumber" name="L109" href="#L109">109</a>             <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">double</strong> value(<strong class="jxr_keyword">double</strong> x) {
<a class="jxr_linenumber" name="L110" href="#L110">110</a>                 <strong class="jxr_keyword">return</strong> (x &gt;= 0 &amp;&amp; x &lt;= 5) ? value : 0.0;
<a class="jxr_linenumber" name="L111" href="#L111">111</a>             }
<a class="jxr_linenumber" name="L112" href="#L112">112</a>         };
<a class="jxr_linenumber" name="L113" href="#L113">113</a>         LegendreGaussIntegrator gauss = <strong class="jxr_keyword">new</strong> LegendreGaussIntegrator(5, 3, 100);
<a class="jxr_linenumber" name="L114" href="#L114">114</a> 
<a class="jxr_linenumber" name="L115" href="#L115">115</a>         <em class="jxr_comment">// due to the discontinuity, integration implies *many* calls</em>
<a class="jxr_linenumber" name="L116" href="#L116">116</a>         <strong class="jxr_keyword">double</strong> maxX = 0.32462367623786328;
<a class="jxr_linenumber" name="L117" href="#L117">117</a>         Assert.assertEquals(maxX * value, gauss.integrate(Integer.MAX_VALUE, f, -10, maxX), 1.0e-7);
<a class="jxr_linenumber" name="L118" href="#L118">118</a>         Assert.assertTrue(gauss.getEvaluations() &gt; 37000000);
<a class="jxr_linenumber" name="L119" href="#L119">119</a>         Assert.assertTrue(gauss.getIterations() &lt; 30);
<a class="jxr_linenumber" name="L120" href="#L120">120</a> 
<a class="jxr_linenumber" name="L121" href="#L121">121</a>         <em class="jxr_comment">// setting up limits prevents such large number of calls</em>
<a class="jxr_linenumber" name="L122" href="#L122">122</a>         <strong class="jxr_keyword">try</strong> {
<a class="jxr_linenumber" name="L123" href="#L123">123</a>             gauss.integrate(1000, f, -10, maxX);
<a class="jxr_linenumber" name="L124" href="#L124">124</a>             Assert.fail(<span class="jxr_string">"expected TooManyEvaluationsException"</span>);
<a class="jxr_linenumber" name="L125" href="#L125">125</a>         } <strong class="jxr_keyword">catch</strong> (TooManyEvaluationsException tmee) {
<a class="jxr_linenumber" name="L126" href="#L126">126</a>             <em class="jxr_comment">// expected</em>
<a class="jxr_linenumber" name="L127" href="#L127">127</a>             Assert.assertEquals(1000, tmee.getMax());
<a class="jxr_linenumber" name="L128" href="#L128">128</a>         }
<a class="jxr_linenumber" name="L129" href="#L129">129</a> 
<a class="jxr_linenumber" name="L130" href="#L130">130</a>         <em class="jxr_comment">// integrating on the two sides should be simpler</em>
<a class="jxr_linenumber" name="L131" href="#L131">131</a>         <strong class="jxr_keyword">double</strong> sum1 = gauss.integrate(1000, f, -10, 0);
<a class="jxr_linenumber" name="L132" href="#L132">132</a>         <strong class="jxr_keyword">int</strong> eval1   = gauss.getEvaluations();
<a class="jxr_linenumber" name="L133" href="#L133">133</a>         <strong class="jxr_keyword">double</strong> sum2 = gauss.integrate(1000, f, 0, maxX);
<a class="jxr_linenumber" name="L134" href="#L134">134</a>         <strong class="jxr_keyword">int</strong> eval2   = gauss.getEvaluations();
<a class="jxr_linenumber" name="L135" href="#L135">135</a>         Assert.assertEquals(maxX * value, sum1 + sum2, 1.0e-7);
<a class="jxr_linenumber" name="L136" href="#L136">136</a>         Assert.assertTrue(eval1 + eval2 &lt; 200);
<a class="jxr_linenumber" name="L137" href="#L137">137</a> 
<a class="jxr_linenumber" name="L138" href="#L138">138</a>     }
<a class="jxr_linenumber" name="L139" href="#L139">139</a> 
<a class="jxr_linenumber" name="L140" href="#L140">140</a>     <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">double</strong> exactIntegration(PolynomialFunction p, <strong class="jxr_keyword">double</strong> a, <strong class="jxr_keyword">double</strong> b) {
<a class="jxr_linenumber" name="L141" href="#L141">141</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong>[] coeffs = p.getCoefficients();
<a class="jxr_linenumber" name="L142" href="#L142">142</a>         <strong class="jxr_keyword">double</strong> yb = coeffs[coeffs.length - 1] / coeffs.length;
<a class="jxr_linenumber" name="L143" href="#L143">143</a>         <strong class="jxr_keyword">double</strong> ya = yb;
<a class="jxr_linenumber" name="L144" href="#L144">144</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = coeffs.length - 2; i &gt;= 0; --i) {
<a class="jxr_linenumber" name="L145" href="#L145">145</a>             yb = yb * b + coeffs[i] / (i + 1);
<a class="jxr_linenumber" name="L146" href="#L146">146</a>             ya = ya * a + coeffs[i] / (i + 1);
<a class="jxr_linenumber" name="L147" href="#L147">147</a>         }
<a class="jxr_linenumber" name="L148" href="#L148">148</a>         <strong class="jxr_keyword">return</strong> yb * b - ya * a;
<a class="jxr_linenumber" name="L149" href="#L149">149</a>     }
<a class="jxr_linenumber" name="L150" href="#L150">150</a> 
<a class="jxr_linenumber" name="L151" href="#L151">151</a> }
