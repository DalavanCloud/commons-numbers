<a class="jxr_linenumber" name="L133" href="#L133">133</a>         <strong class="jxr_keyword">try</strong> {
<a class="jxr_linenumber" name="L134" href="#L134">134</a>             <em class="jxr_comment">// no bracketing</em>
<a class="jxr_linenumber" name="L135" href="#L135">135</a>             solver.solve(100, f, 2, 3);
