<a class="jxr_linenumber" name="L178" href="#L178">178</a>             <strong class="jxr_keyword">double</strong> accum3 = 0.0;
<a class="jxr_linenumber" name="L179" href="#L179">179</a>             <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = begin; i &lt; begin + length; i++) {
