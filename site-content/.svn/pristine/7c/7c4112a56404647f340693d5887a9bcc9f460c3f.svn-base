<a class="jxr_linenumber" name="L166" href="#L166">166</a>         <strong class="jxr_keyword">double</strong> kurt = Double.NaN;
<a class="jxr_linenumber" name="L167" href="#L167">167</a> 
<a class="jxr_linenumber" name="L168" href="#L168">168</a>         <strong class="jxr_keyword">if</strong> (test(values, begin, length) &amp;&amp; length &gt; 3) {
<a class="jxr_linenumber" name="L169" href="#L169">169</a> 
<a class="jxr_linenumber" name="L170" href="#L170">170</a>             <em class="jxr_comment">// Compute the mean and standard deviation</em>
<a class="jxr_linenumber" name="L171" href="#L171">171</a>             <a href="../../../../../../../org/apache/commons/math3/stat/descriptive/moment/Variance.html">Variance</a> variance = <strong class="jxr_keyword">new</strong> <a href="../../../../../../../org/apache/commons/math3/stat/descriptive/moment/Variance.html">Variance</a>();
<a class="jxr_linenumber" name="L172" href="#L172">172</a>             variance.incrementAll(values, begin, length);
<a class="jxr_linenumber" name="L173" href="#L173">173</a>             <strong class="jxr_keyword">double</strong> mean = variance.moment.m1;
<a class="jxr_linenumber" name="L174" href="#L174">174</a>             <strong class="jxr_keyword">double</strong> stdDev = FastMath.sqrt(variance.getResult());
<a class="jxr_linenumber" name="L175" href="#L175">175</a> 
