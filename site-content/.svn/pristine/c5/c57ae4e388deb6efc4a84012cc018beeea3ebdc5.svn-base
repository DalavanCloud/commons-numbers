<a class="jxr_linenumber" name="L338" href="#L338">338</a>                     SparseGradient sgY = SparseGradient.createVariable(1, y);
<a class="jxr_linenumber" name="L339" href="#L339">339</a>                     <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> z = 0.1; z &lt; 1.2; z += 0.1) {
<a class="jxr_linenumber" name="L340" href="#L340">340</a>                         SparseGradient sgZ = SparseGradient.createVariable(2, z);
<a class="jxr_linenumber" name="L341" href="#L341">341</a>                         SparseGradient f = sgX.divide(sgY.cos().add(sgZ.tan())).sin();
<a class="jxr_linenumber" name="L342" href="#L342">342</a>                         <strong class="jxr_keyword">double</strong> a = FastMath.cos(y) + FastMath.tan(z);
<a class="jxr_linenumber" name="L343" href="#L343">343</a>                         <strong class="jxr_keyword">double</strong> f0 = FastMath.sin(x / a);
<a class="jxr_linenumber" name="L344" href="#L344">344</a>                         Assert.assertEquals(f0, f.getValue(), FastMath.abs(epsilon * f0));
<a class="jxr_linenumber" name="L345" href="#L345">345</a>                         <strong class="jxr_keyword">double</strong> dfdx = FastMath.cos(x / a) / a;
<a class="jxr_linenumber" name="L346" href="#L346">346</a>                         Assert.assertEquals(dfdx, f.getDerivative(0), FastMath.abs(epsilon * dfdx));
<a class="jxr_linenumber" name="L347" href="#L347">347</a>                         <strong class="jxr_keyword">double</strong> dfdy =  x * FastMath.sin(y) * dfdx / a;
<a class="jxr_linenumber" name="L348" href="#L348">348</a>                         Assert.assertEquals(dfdy, f.getDerivative(1), FastMath.abs(epsilon * dfdy));
<a class="jxr_linenumber" name="L349" href="#L349">349</a>                         <strong class="jxr_keyword">double</strong> cz = FastMath.cos(z);
<a class="jxr_linenumber" name="L350" href="#L350">350</a>                         <strong class="jxr_keyword">double</strong> cz2 = cz * cz;
<a class="jxr_linenumber" name="L351" href="#L351">351</a>                         <strong class="jxr_keyword">double</strong> dfdz = -x * dfdx / (a * cz2);
<a class="jxr_linenumber" name="L352" href="#L352">352</a>                         Assert.assertEquals(dfdz, f.getDerivative(2), FastMath.abs(epsilon * dfdz));
<a class="jxr_linenumber" name="L353" href="#L353">353</a>                     }
<a class="jxr_linenumber" name="L354" href="#L354">354</a>                 }
<a class="jxr_linenumber" name="L355" href="#L355">355</a>             }
