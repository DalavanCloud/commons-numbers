<a class="jxr_linenumber" name="L334" href="#L334">334</a>         <strong class="jxr_keyword">double</strong> epsilon = 2.0e-12;
<a class="jxr_linenumber" name="L335" href="#L335">335</a>             <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> x = 0.1; x &lt; 1.2; x += 0.1) {
