<a class="jxr_linenumber" name="L242" href="#L242">242</a>                                                       x.multiply(y).multiply(z));
<a class="jxr_linenumber" name="L243" href="#L243">243</a> 
<a class="jxr_linenumber" name="L244" href="#L244">244</a>             <strong class="jxr_keyword">for</strong> (SparseGradient sg : list) {
<a class="jxr_linenumber" name="L245" href="#L245">245</a>                 <em class="jxr_comment">// the special case a = 0 is included here</em>
<a class="jxr_linenumber" name="L246" href="#L246">246</a>                 <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> a : <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { 0.0, 0.1, 1.0, 2.0, 5.0 }) {
