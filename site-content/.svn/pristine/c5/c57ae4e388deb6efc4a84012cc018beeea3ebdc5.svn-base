<a class="jxr_linenumber" name="L287" href="#L287">287</a>     @Test
<a class="jxr_linenumber" name="L288" href="#L288">288</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testExpression() {
<a class="jxr_linenumber" name="L289" href="#L289">289</a>         <strong class="jxr_keyword">double</strong> epsilon = 2.5e-13;
<a class="jxr_linenumber" name="L290" href="#L290">290</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> x = 0; x &lt; 2; x += 0.2) {
<a class="jxr_linenumber" name="L291" href="#L291">291</a>             SparseGradient sgX = SparseGradient.createVariable(0, x);
<a class="jxr_linenumber" name="L292" href="#L292">292</a>             <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> y = 0; y &lt; 2; y += 0.2) {
<a class="jxr_linenumber" name="L293" href="#L293">293</a>                 SparseGradient sgY = SparseGradient.createVariable(1, y);
<a class="jxr_linenumber" name="L294" href="#L294">294</a>                 <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> z = 0; z &gt;- 2; z -= 0.2) {
<a class="jxr_linenumber" name="L295" href="#L295">295</a>                     SparseGradient sgZ = SparseGradient.createVariable(2, z);
<a class="jxr_linenumber" name="L296" href="#L296">296</a> 
<a class="jxr_linenumber" name="L297" href="#L297">297</a>                     <em class="jxr_comment">// f(x, y, z) = x + 5 x y - 2 z + (8 z x - y)^3</em>
<a class="jxr_linenumber" name="L298" href="#L298">298</a>                     SparseGradient sg =
<a class="jxr_linenumber" name="L299" href="#L299">299</a>                             sgZ.linearCombination(1, sgX,
<a class="jxr_linenumber" name="L300" href="#L300">300</a>                                                   5, sgX.multiply(sgY),
<a class="jxr_linenumber" name="L301" href="#L301">301</a>                                                  -2, sgZ,
<a class="jxr_linenumber" name="L302" href="#L302">302</a>                                                  1, sgZ.linearCombination(8, sgZ.multiply(sgX), -1, sgY).pow(3));
<a class="jxr_linenumber" name="L303" href="#L303">303</a>                     <strong class="jxr_keyword">double</strong> f = x + 5 * x * y - 2 * z + FastMath.pow(8 * z * x - y, 3);
<a class="jxr_linenumber" name="L304" href="#L304">304</a>                     Assert.assertEquals(f, sg.getValue(), FastMath.abs(epsilon * f));
<a class="jxr_linenumber" name="L305" href="#L305">305</a> 
<a class="jxr_linenumber" name="L306" href="#L306">306</a>                     <em class="jxr_comment">// df/dx = 1 + 5 y + 24 (8 z x - y)^2 z</em>
<a class="jxr_linenumber" name="L307" href="#L307">307</a>                     <strong class="jxr_keyword">double</strong> dfdx = 1 + 5 * y + 24 * z * FastMath.pow(8 * z * x - y, 2);
<a class="jxr_linenumber" name="L308" href="#L308">308</a>                     Assert.assertEquals(dfdx, sg.getDerivative(0), FastMath.abs(epsilon * dfdx));
<a class="jxr_linenumber" name="L309" href="#L309">309</a> 
<a class="jxr_linenumber" name="L310" href="#L310">310</a>                 }
