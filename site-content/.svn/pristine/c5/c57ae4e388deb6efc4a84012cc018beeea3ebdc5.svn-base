<a class="jxr_linenumber" name="L235" href="#L235">235</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> maxOrder = 1; maxOrder &lt; 5; ++maxOrder) {
<a class="jxr_linenumber" name="L236" href="#L236">236</a> 
<a class="jxr_linenumber" name="L237" href="#L237">237</a>             SparseGradient x = SparseGradient.createVariable(0, 0.1);
