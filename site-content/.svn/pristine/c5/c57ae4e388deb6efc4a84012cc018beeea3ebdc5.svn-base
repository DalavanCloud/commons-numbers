<a class="jxr_linenumber" name="L250" href="#L250">250</a>                     SparseGradient result = SparseGradient.pow(a, sg);
<a class="jxr_linenumber" name="L251" href="#L251">251</a>                     Assert.assertEquals(reference, result);
<a class="jxr_linenumber" name="L252" href="#L252">252</a>                 }
<a class="jxr_linenumber" name="L253" href="#L253">253</a> 
<a class="jxr_linenumber" name="L254" href="#L254">254</a>             }
<a class="jxr_linenumber" name="L255" href="#L255">255</a> 
<a class="jxr_linenumber" name="L256" href="#L256">256</a>             <em class="jxr_comment">// negative base: -1^x can be evaluated for integers only, so value is sometimes OK, derivatives are always NaN</em>
<a class="jxr_linenumber" name="L257" href="#L257">257</a>             SparseGradient negEvenInteger = SparseGradient.pow(-2.0, SparseGradient.createVariable(0, 2.0));
<a class="jxr_linenumber" name="L258" href="#L258">258</a>             Assert.assertEquals(4.0, negEvenInteger.getValue(), 1.0e-15);
<a class="jxr_linenumber" name="L259" href="#L259">259</a>             Assert.assertTrue(Double.isNaN(negEvenInteger.getDerivative(0)));
<a class="jxr_linenumber" name="L260" href="#L260">260</a>             SparseGradient negOddInteger = SparseGradient.pow(-2.0, SparseGradient.createVariable(0, 3.0));
<a class="jxr_linenumber" name="L261" href="#L261">261</a>             Assert.assertEquals(-8.0, negOddInteger.getValue(), 1.0e-15);
<a class="jxr_linenumber" name="L262" href="#L262">262</a>             Assert.assertTrue(Double.isNaN(negOddInteger.getDerivative(0)));
<a class="jxr_linenumber" name="L263" href="#L263">263</a>             SparseGradient negNonInteger = SparseGradient.pow(-2.0, SparseGradient.createVariable(0, 2.001));
<a class="jxr_linenumber" name="L264" href="#L264">264</a>             Assert.assertTrue(Double.isNaN(negNonInteger.getValue()));
<a class="jxr_linenumber" name="L265" href="#L265">265</a>             Assert.assertTrue(Double.isNaN(negNonInteger.getDerivative(0)));
<a class="jxr_linenumber" name="L266" href="#L266">266</a> 
<a class="jxr_linenumber" name="L267" href="#L267">267</a>             SparseGradient zeroNeg = SparseGradient.pow(0.0, SparseGradient.createVariable(0, -1.0));
<a class="jxr_linenumber" name="L268" href="#L268">268</a>             Assert.assertTrue(Double.isNaN(zeroNeg.getValue()));
<a class="jxr_linenumber" name="L269" href="#L269">269</a>             Assert.assertTrue(Double.isNaN(zeroNeg.getDerivative(0)));
<a class="jxr_linenumber" name="L270" href="#L270">270</a>             SparseGradient posNeg = SparseGradient.pow(2.0, SparseGradient.createVariable(0, -2.0));
<a class="jxr_linenumber" name="L271" href="#L271">271</a>             Assert.assertEquals(1.0 / 4.0, posNeg.getValue(), 1.0e-15);
<a class="jxr_linenumber" name="L272" href="#L272">272</a>             Assert.assertEquals(FastMath.log(2.0) / 4.0, posNeg.getDerivative(0), 1.0e-15);
<a class="jxr_linenumber" name="L273" href="#L273">273</a> 
<a class="jxr_linenumber" name="L274" href="#L274">274</a>             <em class="jxr_comment">// very special case: a = 0 and power = 0</em>
<a class="jxr_linenumber" name="L275" href="#L275">275</a>             SparseGradient zeroZero = SparseGradient.pow(0.0, SparseGradient.createVariable(0, 0.0));
<a class="jxr_linenumber" name="L276" href="#L276">276</a> 
<a class="jxr_linenumber" name="L277" href="#L277">277</a>             <em class="jxr_comment">// this should be OK for simple first derivative with one variable only ...</em>
