<a class="jxr_linenumber" name="L784" href="#L784">784</a>                 <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> z = 0; z &lt; 1.2; z += 0.2) {
<a class="jxr_linenumber" name="L785" href="#L785">785</a>                     SparseGradient sgZ = SparseGradient.createVariable(2, z);
<a class="jxr_linenumber" name="L786" href="#L786">786</a>                     SparseGradient f = sgX.multiply(3).add(sgZ.multiply(-2)).add(sgY.multiply(5));
<a class="jxr_linenumber" name="L787" href="#L787">787</a>                     <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> dx = -0.2; dx &lt; 0.2; dx += 0.2) {
<a class="jxr_linenumber" name="L788" href="#L788">788</a>                         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> dy = -0.2; dy &lt; 0.2; dy += 0.1) {
<a class="jxr_linenumber" name="L789" href="#L789">789</a>                             <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> dz = -0.2; dz &lt; 0.2; dz += 0.1) {
<a class="jxr_linenumber" name="L790" href="#L790">790</a>                                 <strong class="jxr_keyword">double</strong> ref = 3 * (x + dx) + 5 * (y + dy) -2 * (z + dz);
<a class="jxr_linenumber" name="L791" href="#L791">791</a>                                 Assert.assertEquals(ref, f.taylor(dx, dy, dz), 3.0e-15);
<a class="jxr_linenumber" name="L792" href="#L792">792</a>                             }
<a class="jxr_linenumber" name="L793" href="#L793">793</a>                         }
