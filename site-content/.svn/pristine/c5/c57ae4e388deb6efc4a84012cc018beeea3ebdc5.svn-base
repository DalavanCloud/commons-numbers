<a class="jxr_linenumber" name="L801" href="#L801">801</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testTaylorAtan2() {
<a class="jxr_linenumber" name="L802" href="#L802">802</a>         <strong class="jxr_keyword">double</strong> x0 =  0.1;
<a class="jxr_linenumber" name="L803" href="#L803">803</a>         <strong class="jxr_keyword">double</strong> y0 = -0.3;
<a class="jxr_linenumber" name="L804" href="#L804">804</a>             SparseGradient sgX   = SparseGradient.createVariable(0, x0);
<a class="jxr_linenumber" name="L805" href="#L805">805</a>             SparseGradient sgY   = SparseGradient.createVariable(1, y0);
<a class="jxr_linenumber" name="L806" href="#L806">806</a>             SparseGradient atan2 = SparseGradient.atan2(sgY, sgX);
<a class="jxr_linenumber" name="L807" href="#L807">807</a>             <strong class="jxr_keyword">double</strong> maxError = 0;
<a class="jxr_linenumber" name="L808" href="#L808">808</a>             <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> dx = -0.05; dx &lt; 0.05; dx += 0.001) {
<a class="jxr_linenumber" name="L809" href="#L809">809</a>                 <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> dy = -0.05; dy &lt; 0.05; dy += 0.001) {
<a class="jxr_linenumber" name="L810" href="#L810">810</a>                     <strong class="jxr_keyword">double</strong> ref = FastMath.atan2(y0 + dy, x0 + dx);
<a class="jxr_linenumber" name="L811" href="#L811">811</a>                     maxError = FastMath.max(maxError, FastMath.abs(ref - atan2.taylor(dx, dy)));
<a class="jxr_linenumber" name="L812" href="#L812">812</a>                 }
<a class="jxr_linenumber" name="L813" href="#L813">813</a>             }
<a class="jxr_linenumber" name="L814" href="#L814">814</a>             <strong class="jxr_keyword">double</strong> expectedError = 0.0241;
<a class="jxr_linenumber" name="L815" href="#L815">815</a>             Assert.assertEquals(expectedError, maxError, 0.01 * expectedError);
<a class="jxr_linenumber" name="L816" href="#L816">816</a>     }
