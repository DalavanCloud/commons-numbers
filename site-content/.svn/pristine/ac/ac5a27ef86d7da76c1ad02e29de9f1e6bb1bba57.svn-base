<a class="jxr_linenumber" name="L47" href="#L47">47</a>      @Test
<a class="jxr_linenumber" name="L48" href="#L48">48</a>      <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testGetFeatures() {
<a class="jxr_linenumber" name="L49" href="#L49">49</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong>[] features = { -1, -1e-97, 0, 23.456, 9.01e203 } ;
<a class="jxr_linenumber" name="L50" href="#L50">50</a>          <strong class="jxr_keyword">final</strong> Neuron n = <strong class="jxr_keyword">new</strong> Neuron(1, features);
<a class="jxr_linenumber" name="L51" href="#L51">51</a>  
<a class="jxr_linenumber" name="L52" href="#L52">52</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong>[] f = n.getFeatures();
<a class="jxr_linenumber" name="L53" href="#L53">53</a>          <em class="jxr_comment">// Accessor returns a copy.</em>
<a class="jxr_linenumber" name="L54" href="#L54">54</a>          Assert.assertFalse(f == features);
<a class="jxr_linenumber" name="L55" href="#L55">55</a>  
<a class="jxr_linenumber" name="L56" href="#L56">56</a>          <em class="jxr_comment">// Values are the same.</em>
<a class="jxr_linenumber" name="L57" href="#L57">57</a>          Assert.assertEquals(features.length, f.length);
<a class="jxr_linenumber" name="L58" href="#L58">58</a>          <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; features.length; i++) {
<a class="jxr_linenumber" name="L59" href="#L59">59</a>              Assert.assertEquals(features[i], f[i], 0d);
<a class="jxr_linenumber" name="L60" href="#L60">60</a>          }
<a class="jxr_linenumber" name="L61" href="#L61">61</a>      }
<a class="jxr_linenumber" name="L62" href="#L62">62</a>  
<a class="jxr_linenumber" name="L63" href="#L63">63</a>      @Test
<a class="jxr_linenumber" name="L64" href="#L64">64</a>      <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testCompareAndSetFeatures() {
<a class="jxr_linenumber" name="L65" href="#L65">65</a>          <strong class="jxr_keyword">final</strong> Neuron n = <strong class="jxr_keyword">new</strong> Neuron(1, <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { 0 });
<a class="jxr_linenumber" name="L66" href="#L66">66</a>          <strong class="jxr_keyword">double</strong>[] expect = n.getFeatures();
<a class="jxr_linenumber" name="L67" href="#L67">67</a>          <strong class="jxr_keyword">double</strong>[] update = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { expect[0] + 1.23 };
<a class="jxr_linenumber" name="L68" href="#L68">68</a>  
<a class="jxr_linenumber" name="L69" href="#L69">69</a>          <em class="jxr_comment">// Test "success".</em>
<a class="jxr_linenumber" name="L70" href="#L70">70</a>          <strong class="jxr_keyword">boolean</strong> ok = n.compareAndSetFeatures(expect, update);
<a class="jxr_linenumber" name="L71" href="#L71">71</a>          <em class="jxr_comment">// Check that the update is reported as successful.</em>
<a class="jxr_linenumber" name="L72" href="#L72">72</a>          Assert.assertTrue(ok);
<a class="jxr_linenumber" name="L73" href="#L73">73</a>          <em class="jxr_comment">// Check that the new value is correct.</em>
<a class="jxr_linenumber" name="L74" href="#L74">74</a>          Assert.assertEquals(update[0],  n.getFeatures()[0], 0d);
<a class="jxr_linenumber" name="L75" href="#L75">75</a>  
<a class="jxr_linenumber" name="L76" href="#L76">76</a>          <em class="jxr_comment">// Test "failure".</em>
<a class="jxr_linenumber" name="L77" href="#L77">77</a>          <strong class="jxr_keyword">double</strong>[] update1 = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { update[0] + 4.56 };
<a class="jxr_linenumber" name="L78" href="#L78">78</a>          <em class="jxr_comment">// Must return "false" because the neuron has been</em>
<a class="jxr_linenumber" name="L79" href="#L79">79</a>          <em class="jxr_comment">// updated: a new update can only succeed if "expect"</em>
<a class="jxr_linenumber" name="L80" href="#L80">80</a>          <em class="jxr_comment">// is set to the new features.</em>
<a class="jxr_linenumber" name="L81" href="#L81">81</a>          ok = n.compareAndSetFeatures(expect, update1);
<a class="jxr_linenumber" name="L82" href="#L82">82</a>          <em class="jxr_comment">// Check that the update is reported as failed.</em>
<a class="jxr_linenumber" name="L83" href="#L83">83</a>          Assert.assertFalse(ok);
<a class="jxr_linenumber" name="L84" href="#L84">84</a>          <em class="jxr_comment">// Check that the value was not changed.</em>
<a class="jxr_linenumber" name="L85" href="#L85">85</a>          Assert.assertEquals(update[0],  n.getFeatures()[0], 0d);
<a class="jxr_linenumber" name="L86" href="#L86">86</a>      }
<a class="jxr_linenumber" name="L87" href="#L87">87</a>  
<a class="jxr_linenumber" name="L88" href="#L88">88</a>      @Test
<a class="jxr_linenumber" name="L89" href="#L89">89</a>      <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testCopy() {
<a class="jxr_linenumber" name="L90" href="#L90">90</a>          <strong class="jxr_keyword">final</strong> Neuron n = <strong class="jxr_keyword">new</strong> Neuron(1, <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { 9.87 });
<a class="jxr_linenumber" name="L91" href="#L91">91</a>  
<a class="jxr_linenumber" name="L92" href="#L92">92</a>          <em class="jxr_comment">// Update original.</em>
<a class="jxr_linenumber" name="L93" href="#L93">93</a>          <strong class="jxr_keyword">double</strong>[] update = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { n.getFeatures()[0] + 2.34 };
<a class="jxr_linenumber" name="L94" href="#L94">94</a>          n.compareAndSetFeatures(n.getFeatures(), update);
<a class="jxr_linenumber" name="L95" href="#L95">95</a>  
<a class="jxr_linenumber" name="L96" href="#L96">96</a>          <em class="jxr_comment">// Create a copy.</em>
<a class="jxr_linenumber" name="L97" href="#L97">97</a>          <strong class="jxr_keyword">final</strong> Neuron copy = n.copy();
<a class="jxr_linenumber" name="L98" href="#L98">98</a>  
<a class="jxr_linenumber" name="L99" href="#L99">99</a>          <em class="jxr_comment">// Check that original and copy have the same value.</em>
<a class="jxr_linenumber" name="L100" href="#L100">100</a>         Assert.assertTrue(n.getFeatures()[0] == copy.getFeatures()[0]);
<a class="jxr_linenumber" name="L101" href="#L101">101</a>         Assert.assertEquals(n.getNumberOfAttemptedUpdates(),
<a class="jxr_linenumber" name="L102" href="#L102">102</a>                             copy.getNumberOfAttemptedUpdates());
<a class="jxr_linenumber" name="L103" href="#L103">103</a> 
<a class="jxr_linenumber" name="L104" href="#L104">104</a>         <em class="jxr_comment">// Update original.</em>
<a class="jxr_linenumber" name="L105" href="#L105">105</a>         update = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { 1.23 * n.getFeatures()[0] };
<a class="jxr_linenumber" name="L106" href="#L106">106</a>         n.compareAndSetFeatures(n.getFeatures(), update);
<a class="jxr_linenumber" name="L107" href="#L107">107</a> 
<a class="jxr_linenumber" name="L108" href="#L108">108</a>         <em class="jxr_comment">// Check that original and copy differ.</em>
<a class="jxr_linenumber" name="L109" href="#L109">109</a>         Assert.assertFalse(n.getFeatures()[0] == copy.getFeatures()[0]);
<a class="jxr_linenumber" name="L110" href="#L110">110</a>         Assert.assertNotEquals(n.getNumberOfSuccessfulUpdates(),
<a class="jxr_linenumber" name="L111" href="#L111">111</a>                                copy.getNumberOfSuccessfulUpdates());
<a class="jxr_linenumber" name="L112" href="#L112">112</a>     }
<a class="jxr_linenumber" name="L113" href="#L113">113</a> 
<a class="jxr_linenumber" name="L114" href="#L114">114</a>     @Test
<a class="jxr_linenumber" name="L115" href="#L115">115</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testSerialize()
<a class="jxr_linenumber" name="L116" href="#L116">116</a>         <strong class="jxr_keyword">throws</strong> IOException,
<a class="jxr_linenumber" name="L117" href="#L117">117</a>                ClassNotFoundException {
<a class="jxr_linenumber" name="L118" href="#L118">118</a>         <strong class="jxr_keyword">final</strong> Neuron out = <strong class="jxr_keyword">new</strong> Neuron(123, <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { -98.76, -1, 0, 1e-23, 543.21, 1e234 });
<a class="jxr_linenumber" name="L119" href="#L119">119</a>         <strong class="jxr_keyword">final</strong> ByteArrayOutputStream bos = <strong class="jxr_keyword">new</strong> ByteArrayOutputStream();
