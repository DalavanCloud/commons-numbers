<a class="jxr_linenumber" name="L130" href="#L130">130</a>         <em class="jxr_comment">// Same values.</em>
<a class="jxr_linenumber" name="L131" href="#L131">131</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong>[] outF = out.getFeatures();
