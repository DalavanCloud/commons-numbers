<a class="jxr_linenumber" name="L1057" href="#L1057">1057</a>         <strong class="jxr_keyword">final</strong> <a href="../../../../../org/apache/commons/math3/linear/RealVector.html">RealVector</a> x = <strong class="jxr_keyword">new</strong> <a href="../../../../../org/apache/commons/math3/linear/ArrayRealVector.html">ArrayRealVector</a>(a.getColumnDimension());
<a class="jxr_linenumber" name="L1058" href="#L1058">1058</a>         <strong class="jxr_keyword">return</strong> solveInPlace(a, <strong class="jxr_keyword">null</strong>, b, x, goodb, shift);
