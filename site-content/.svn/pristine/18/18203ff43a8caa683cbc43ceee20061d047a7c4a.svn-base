<a class="jxr_linenumber" name="L609" href="#L609">609</a>             <strong class="jxr_keyword">this</strong>.cgnorm = <strong class="jxr_keyword">this</strong>.beta1;
<a class="jxr_linenumber" name="L610" href="#L610">610</a>             <strong class="jxr_keyword">this</strong>.gbar = alpha;
<a class="jxr_linenumber" name="L611" href="#L611">611</a>             <strong class="jxr_keyword">this</strong>.dbar = <strong class="jxr_keyword">this</strong>.beta;
<a class="jxr_linenumber" name="L612" href="#L612">612</a>             <strong class="jxr_keyword">this</strong>.gammaZeta = <strong class="jxr_keyword">this</strong>.beta1;
<a class="jxr_linenumber" name="L613" href="#L613">613</a>             <strong class="jxr_keyword">this</strong>.minusEpsZeta = 0.;
