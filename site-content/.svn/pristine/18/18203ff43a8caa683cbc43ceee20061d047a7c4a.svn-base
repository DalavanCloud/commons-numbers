<a class="jxr_linenumber" name="L899" href="#L899">899</a> <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L900" href="#L900">900</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">boolean</strong> getCheck() {
