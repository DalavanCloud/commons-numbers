<a class="jxr_linenumber" name="L76" href="#L76">76</a>  <em class="jxr_javadoccomment">   * @param interpolator interpolator to copy from. The copy is a deep</em>
<a class="jxr_linenumber" name="L77" href="#L77">77</a>  <em class="jxr_javadoccomment">   * copy: its arrays are separated from the original arrays of the</em>
<a class="jxr_linenumber" name="L78" href="#L78">78</a>  <em class="jxr_javadoccomment">   * instance</em>
<a class="jxr_linenumber" name="L79" href="#L79">79</a>  <em class="jxr_javadoccomment">   */</em>
<a class="jxr_linenumber" name="L80" href="#L80">80</a>    <strong class="jxr_keyword">public</strong> <a href="../../../../../../org/apache/commons/math3/ode/sampling/DummyStepInterpolator.html">DummyStepInterpolator</a>(<strong class="jxr_keyword">final</strong> <a href="../../../../../../org/apache/commons/math3/ode/sampling/DummyStepInterpolator.html">DummyStepInterpolator</a> interpolator) {
<a class="jxr_linenumber" name="L81" href="#L81">81</a>      <strong class="jxr_keyword">super</strong>(interpolator);
<a class="jxr_linenumber" name="L82" href="#L82">82</a>      <strong class="jxr_keyword">if</strong> (interpolator.currentDerivative != <strong class="jxr_keyword">null</strong>) {
