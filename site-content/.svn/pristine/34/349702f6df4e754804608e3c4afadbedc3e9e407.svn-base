<a class="jxr_linenumber" name="L115" href="#L115">115</a>     <strong class="jxr_keyword">throws</strong> IOException {
<a class="jxr_linenumber" name="L116" href="#L116">116</a> 
<a class="jxr_linenumber" name="L117" href="#L117">117</a>       <em class="jxr_comment">// save the state of the base class</em>
<a class="jxr_linenumber" name="L118" href="#L118">118</a>     writeBaseExternal(out);
<a class="jxr_linenumber" name="L119" href="#L119">119</a> 
<a class="jxr_linenumber" name="L120" href="#L120">120</a>     <strong class="jxr_keyword">if</strong> (currentDerivative != <strong class="jxr_keyword">null</strong>) {
<a class="jxr_linenumber" name="L121" href="#L121">121</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; currentDerivative.length; ++i) {
