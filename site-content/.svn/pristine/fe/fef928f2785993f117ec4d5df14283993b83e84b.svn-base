<a class="jxr_linenumber" name="L323" href="#L323">323</a>         Arrays.fill(<strong class="jxr_keyword">this</strong>.x_sing, 0.0);
<a class="jxr_linenumber" name="L324" href="#L324">324</a>         Arrays.fill(<strong class="jxr_keyword">this</strong>.lindep, false);
<a class="jxr_linenumber" name="L325" href="#L325">325</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; nvars; i++) {
<a class="jxr_linenumber" name="L326" href="#L326">326</a>             <strong class="jxr_keyword">this</strong>.vorder[i] = i;
<a class="jxr_linenumber" name="L327" href="#L327">327</a>         }
<a class="jxr_linenumber" name="L328" href="#L328">328</a>         <strong class="jxr_keyword">this</strong>.nobs = 0;
<a class="jxr_linenumber" name="L329" href="#L329">329</a>         <strong class="jxr_keyword">this</strong>.sserr = 0.0;
<a class="jxr_linenumber" name="L330" href="#L330">330</a>         <strong class="jxr_keyword">this</strong>.sumy = 0.0;
<a class="jxr_linenumber" name="L331" href="#L331">331</a>         <strong class="jxr_keyword">this</strong>.sumsqy = 0.0;
<a class="jxr_linenumber" name="L332" href="#L332">332</a>         <strong class="jxr_keyword">this</strong>.rss_set = false;
<a class="jxr_linenumber" name="L333" href="#L333">333</a>         <strong class="jxr_keyword">this</strong>.tol_set = false;
<a class="jxr_linenumber" name="L334" href="#L334">334</a>     }
